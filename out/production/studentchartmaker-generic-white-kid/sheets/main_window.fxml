<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Text?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="sheets.MainWindowController"
            prefHeight="400.0" prefWidth="600.0">

    <children>
        <VBox fx:id="outside">
            <MenuBar fx:id="menu">
                <menus>
                    <Menu fx:id="File" text="File">
                        <items>
                            <MenuItem fx:id="filePick" text="Select Config File"></MenuItem>
                            <MenuItem fx:id="saveToPDF" text="Save to PDF"></MenuItem>
                            <MenuItem fx:id="options" text="Print Options"></MenuItem>
                        </items>
                    </Menu>
                </menus>
            </MenuBar>
            <TableView fx:id="view"></TableView>
        </VBox>
    </children>

</AnchorPane>
